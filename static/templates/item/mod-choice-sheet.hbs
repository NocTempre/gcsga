<form autocomplete="off">
	<div class="fieldblock">
	{{#each choices as |v k|}}
	<div class="field">
		<input type="checkbox" name="{{k}}" value="{{v.enabled}}" data-dtype="String"/>
	</div>
	<label>{{v.name}}{{#if v.notes}} ({{v.notes}}){{/if}}</label>
	{{/each}}
	</div>
	<div class="buttons">
		<button id="apply">{{localize "gurps.item.substitution.apply"}}</button>
		<button id="cancel">{{localize "gurps.item.substitution.cancel"}}</button>
	</div>
</form>
