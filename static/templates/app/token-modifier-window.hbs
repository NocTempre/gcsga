{{#if selfModifiers}}
	<div class="list">
		<header>{{localize "gurps.token_modifier_window.self_modifiers"}}</header>
		{{#each selfModifiers}}
			<div class="modifier rollable">{{signed this.modifier}}</div>
			<div class="name">{{this.name}}</div>
		{{/each}}
	</div>
{{/if}}
{{#if targetModifiers}}
	<div class="list">
		<header>{{localize "gurps.token_modifier_window.target_modifiers"}}</header>
		{{#each targetModifiers}}
			<div class="modifier rollable">{{signed this.modifier}}</div>
			<div class="name">{{this.name}}</div>
		{{/each}}
	</div>
{{/if}}
{{#if targets}}
	{{#each targets}}
		<div class="list">
			<header>{{localize "gurps.token_modifier_window.target_name" name=this.name}}</header>
			{{#each this.targetModifiers}}
				<div class="modifier rollable">{{signed this.modifier}}</div>
				<div class="name">{{this.name}}</div>
			{{/each}}
		</div>
	{{/each}}
{{/if}}
{{#unless (or selfModifiers targetModifiers targets)}}
	<div>{{localize "gurps.token_modifier_window.no_targets"}}</div>
{{/unless}}