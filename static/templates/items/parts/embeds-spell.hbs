<div class="items-section" data-types="{{join system.childTypes ','}}">
	<div class="items-header">
		<div class="item-header item-dropdown"></div>
		<div class="item-header item-name">Name</div>
		<div class="item-header item-college">College</div>
		<div class="item-header item-resist">Resist</div>
		<div class="item-header item-spell-class">Class</div>
		<div class="item-header item-casting-cost">Cast</div>
		<div class="item-header item-maintenance-cost">Maintenance</div>
		<div class="item-header item-casting-time">Time</div>
		<div class="item-header item-duration">Duration</div>
		<div class="item-header item-difficulty">Diff</div>
		<div class="item-header item-prereq-count">P#</div>
		<div class="item-header item-controls"></div>
	</div>
	<ul class="item-list">
		{{#each children}} {{> "gurps.embeds-entry" item=this}} {{/each}}
	</ul>
</div>
